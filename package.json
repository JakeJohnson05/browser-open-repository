    "inquirer-exit-listener": "github:JakeJohnson05/InquirerExitListener",
    "ipt": "^3.0.3",
    "simple-output": "^2.1.1",
    "text-table": "^0.2.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "ava": "^3.8.1",
    "tsd": "^0.13.1",
